{
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
        "acquireVsCodeApi": true,
        "document": true,
        "console": true
    },
    "rules": {
        "no-console": 0
    }
}